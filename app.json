      "package": "com.thuanthien.expensestrackerapp",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5bde18a4-20af-4c49-831d-0b324fef5eb1"
